{
  "manifest_version": 3,
    "name": "TabSaver",
    "version": "1.0",
    "description": "This is a simple yet useful extension that allows you to save tabs and links for later use.",
    "permissions": [
      "tabs",
      "storage"
    ],
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
    "action": {
      "default_popup": "index.html"
    }
}